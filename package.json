{
  "name": "ai-build-server",
  "version": "0.2.12",
  "scripts": {
    "ng": "ng",
    "start": "node dist/index.js",
    "clean": "rimraf dist",
    "watch": "concurrently -k -p \"[{name}]\" -n \"Client,Server\" -c \"yellow.bold,cyan.bold,green.bold\" \"npm run client:watch\" \"npm run server:watch\" ",
    "build": "npm run clean && npm run client:build && npm run server:build",
    "build:prod": "npm run clean && npm run client:build:prod && npm run server:build",
    "client:build": "cd client && npm run build",
    "client:build:prod": "cd client && npm run build:prod",
    "client:watch": "cd client && npm run watch",
    "server:build": "npm run server:build-ts && npm run copy:html && npm run server:tslint",
    "server:build-ts": "tsc -p ./server/tsconfig.json",
    "server:watch": "tsc -p ./server/tsconfig.json -w",
    "server:tslint": "tslint -c ./server/tslint.json -p ./server/tsconfig.json",
    "debug": "npm run server:build",
    "copy:html": "cpx \"server/modules/**/*.html\" dist/modules",
    "client:lib": "cd client && npm run lib:build",
    "client:lib:version": "cd client && npm run lib:version",
    "client:lib:watch": "cd client && npm run lib:watch",
    "client:lib:publish": "cd client && npm run lib:publish",
    "core:publish-lib": "./node_modules/@aitheon/core-server/publish-lib.sh",
    "core:deploy": "NPM_CLIENT_LIB_NAME=build-server ./node_modules/@aitheon/core-server/deploy.sh",
    "core:build": "./node_modules/@aitheon/core-server/build.sh",
    "core:init": "./node_modules/@aitheon/core-server/init.sh",
    "core:generate-rest": "./node_modules/@aitheon/core-server/generate-rest.sh"
  },
  "private": true,
  "dependencies": {
    "@aitheon/core-server": "3.0.1",
    "@aitheon/creators-studio-server": "1.48.0",
    "@aitheon/system-graph-server": "1.118.0",
    "@aitheon/transporter": "3.0.1",
    "aws-sdk": "2.433.0",
    "concurrently": "4.1.0",
    "elasticsearch": "16.7.1",
    "generate-password": "1.4.2",
    "http-aws-es": "6.0.0",
    "ioredis": "4.9.0",
    "json-stream": "1.0.0",
    "kubernetes-client": "6.11.1",
    "nodegit": "0.24.1",
    "semver": "6.0.0",
    "shelljs": "^0.8.3",
    "slug": "1.1.0",
    "websocket": "^1.0.32",
    "yamljs": "0.3.0"
  },
  "devDependencies": {
    "@types/elasticsearch": "5.0.36",
    "@types/ioredis": "4.0.10",
    "@types/nodegit": "0.24.6",
    "@types/semver": "6.0.0",
    "@types/shelljs": "^0.8.4",
    "@types/slug": "0.9.1",
    "@types/websocket": "^1.0.1",
    "@types/yamljs": "0.2.30"
  }
}
